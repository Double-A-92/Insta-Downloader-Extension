{
    "name": "Instagram Downloader",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "One click and Instagram will get download button",
    "homepage_url": "http://aashutoshrathi.co",
    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "icons": {
      "16": "img/icon-16x16.png",
      "32": "img/icon-32x32.png",
      "128": "img/icon-128x128.png"
    },

    "browser_action": {
        "default_title": "Click Me"
    },

    "permissions": [
        "storage",
        "tabs",
        "https://www.instagram.com/"
    ]
}